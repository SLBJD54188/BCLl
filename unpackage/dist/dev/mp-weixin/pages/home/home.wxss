/* 水平间距 */
/* 水平间距 */
.swiper_image {
  width: 100%;
}
.classify {
  margin: 15px 0;
  display: flex;
  overflow: hidden;
  text-align: center;
}
.classify view {
  flex: 1;
}
.classify view image {
  width: 50px;
  height: 50px;
}
._h1 {
  height: 40px;
  line-height: 20px;
  font-weight: 800;
  margin: 20px 0;
  text-align: center;
}
.footer {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.footer view {
  font-size: 10px;
  flex: 45%;
  display: flex;
  text-align: center;
  align-items: center;
  margin: 20px 5px;
}
.footer view image {
  width: 80px;
  height: 80px;
  margin-left: 20px 0;
}

